{
  "name": "twoot",
  "version": "0.0.4",
  "description": "dead simple tweets and toots.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "prepublishOnly": "npm run build",
    "test": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "author": "s <https://github.com/lostfictions>",
  "homepage": "https://github.com/lostfictions/twoot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lostfictions/twoot.git"
  },
  "bugs": {
    "url": "https://github.com/lostfictions/twoot/issues"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "form-data": "^2.3.1",
    "got": "^9.6.0",
    "twit": "^2.2.9"
  },
  "devDependencies": {
    "@types/form-data": "^2.2.1",
    "@types/got": "^9.4.4",
    "@types/node": "^12.0.3",
    "@types/twit": "^2.2.9",
    "typescript": "^3.5.1"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
